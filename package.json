{
  "name": "trainznation_app_tf2",
  "version": "0.0.18",
  "productName": "Trainznation Mod Launcher",
  "description": "Launcher et téléchargement de mod Transport Fever 2 par Trainznation Studio",
  "main": "app/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --debug",
    "watch": "npm run development -- --watch",
    "dist": "cross-env ELECTRON_BUILDER_ALLOW_UNRESOLVED_DEPENDENCIES=true node build.js",
    "build": "electron-builder build",
    "publish": "electron-builder build -p always",
    "lint": "eslint --config .eslintrc.json .",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trainznation/trainznation_app_tf2.git"
  },
  "keywords": [
    "trainznation",
    "mod",
    "transport",
    "fever",
    "2",
    "transport",
    "gestion",
    "launcher",
    "download"
  ],
  "author": "Trainznation Studio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trainznation/trainznation_app_tf2/issues"
  },
  "homepage": "https://github.com/trainznation/trainznation_app_tf2#readme",
  "engines": {
    "node": "14.x.x"
  },
  "dependencies": {
    "@electron/remote": "^1.0.4",
    "@splidejs/splide": "^2.4.21",
    "adm-zip": "^0.5.3",
    "async": "^3.2.0",
