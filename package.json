    "electron-updater": "^4.3.8",
    "electron-util": "^0.14.2",
    "fs-extra": "^9.1.0",
    "github-syntax-dark": "^0.5.0",
    "jquery": "^3.6.0",
    "moment": "^2.29.1",
    "new-github-issue-url": "^0.2.1",
    "node-fetch": "^2.6.1",
    "notyf": "^3.9.0",
    "request": "^2.88.2",
    "semver": "^7.3.5",
    "systeminformation": "^5.6.8",
